{
    "name": "ets-log210-typescript-node-api",
    "version": "1.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start2": "apidoc -i src/routes/ -o dist/docs/ & node dist/index.js & open http://localhost:3200",
        "start": "apidoc -i src/routes/ -o dist/docs/ & node dist/index.js & open http://127.0.0.1:3200",
        "start-docker": "apidoc -i src/routes/ -o dist/docs/ & node dist/index.js",
        "test": "jest --coverage",
        "watch": "jest --coverage --watchAll",
        "docs": "apidoc -i src/routes/ -o dist/docs/",
        "tplant": "tplant --input src/App.ts  -A --output dist/docs/dcl.svg",
        "circular": "madge src/App.ts -c",
        "madge": "madge src/App.ts -i docs/madge.png",
        "precompile": "npm run clean",
        "clean": "rimraf dist/* & npm run docs & npm run tplant",
        "compile": "npm run compile:ts",
        "compile:ts": "tsc && tsc -p tests",
        "postcompile": "cp src/model/model/MDD.svg dist/docs & npm run madge & cp docs/madge.png dist/docs"
    },
    "watch": {
        "test": "{src,tests}/*.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/chai": "^4.2.21",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.1",
        "@types/md5": "^2.3.1",
        "@types/morgan": "^1.9.3",
        "@types/multimap": "^1.1.2",
        "apidoc": "^0.29.0",
        "chai": "^4.3.4",
        "chai-http": "^4.3.0",
        "coveralls": "^3.1.1",
        "debug": "^4.3.2",
        "jest": "^27.2.0",
        "jest-config": "^27.2.0",
        "madge": "^5.0.1",
        "npm-watch": "^0.11.0",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.2",
        "system-sleep": "^1.3.7",
        "testdouble": "^3.16.2",
        "testdouble-chai": "^0.5.0",
        "ts-jest": "^27.0.5",
        "ts-mockito": "^2.6.1",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "express": "^4.17.1",
        "md5": "^2.3.0",
        "morgan": "^1.10.0",
        "multimap": "^1.1.0",
        "tplant": "^3.1.0"
    }
}
